
.container{
    width: 100%;
    background: #FFFFFF;

}
.swiper{
    width: 100%;
    height: 518rpx;
}

.container-info{
    display:flex;
    flex-direction:row;
}
.container-title{
    display:flex;
    flex-direction:column;
    margin: 30rpx 28rpx;

}
.container-price{
    display:flex;
    flex-direction:column;
    top:380rpx;
    position: absolute;
    left: 538rpx;
}

.image-price image{
    width: 184rpx;
    height: 218rpx;
    margin-right: 28rpx;
}
.text-price{
    position: absolute;
    padding-left: 36rpx;
    top: 88rpx;
}
.weight-txt{
    font-size: 32rpx;
    font-weight: 500;
    font-family: PingFangSC, PingFang SC,sans-serif;
    line-height: 17px;
}
.light-txt{
    font-size: 24rpx;
    font-weight: 400;
    color: #999999;
    line-height: 17px;
}

.price-unit{
    padding-left: 6rpx;
    font-size: 24rpx !important;
    width: 24rpx;
    height: 34rpx;
    font-family: PingFangSC, PingFang SC,sans-serif;
    font-weight: 400;
    color: #ffffff !important;

}
.price-value{
    font-weight: 500;
    width: 78rpx;
    height: 78rpx;
    font-size: 54rpx !important;
    font-family: YouSheBiaoTiHei,sans-serif;
    color: #ffffff !important;
    border-radius: 50rpx !important;
}
.container-base{
    display:flex;
    flex-direction:column;
    background: #FFFFFF;
    height: 242rpx;
    margin-top: 20rpx;
}
.container-base-child{
    display:flex;
    flex-direction:column;
    margin: 28rpx 10rpx;

}
.base-time{
    display:flex;
    flex-direction:row;
}
.base-time text{
    font-size: 28rpx;
    font-family: PingFangSC, PingFang SC,sans-serif;
    font-weight: 400;
    color: #000000;
    line-height: 34rpx;
}
.base-address{
    display:flex;
    flex-direction:row;
}
.base-address text{
    font-size: 28rpx;
    font-family: PingFangSC, PingFang SC,sans-serif;
    font-weight: 400;
    color: #999999;
    line-height: 34rpx;

}
.base-phone{
    display:flex;
    flex-direction:row;
    align-items: center;
}
.base-phone text{
    font-size: 24rpx;
    font-family: PingFangSC, PingFang SC,sans-serif;
    font-weight: 400;
    color: #999999;
    line-height: 34rpx;
}
.container-base-child image{
    width: 36rpx;
    height: 36rpx;
    margin-left: 28rpx;
    margin-right: 10rpx;
}

.hr-notice{
    margin: 10rpx 10rpx;
    width: 690rpx;
    height: 2rpx;
    background: #E6E6E6;
}
.hr{
    margin: 18rpx 10rpx;
    width: 690rpx;
    height: 2rpx;
    background: #E6E6E6;
}

.container-notice{
    display:flex;
    flex-direction:column;
    background: #FFFFFF;
    margin-top: 20rpx;
}
.container-notice image{
    width: 24rpx;
    height: 26rpx;
    margin-right: 10rpx;
}
.notice-img{
    display:flex;
    flex-direction:row;
    width: 300rpx;
    margin: 10rpx 28rpx;
    align-items: center;
}
.notice-img text{
    font-size: 28rpx;
    font-family: PingFangSC, PingFang SC,sans-serif;
    font-weight: 500;
    color: #000000;
}
.notice-doc{
    margin: 10rpx 48rpx;
    font-size: 24rpx;
    font-family: PingFangSC, PingFang SC,sans-serif;
    font-weight: 400;
    color: #333333;
    line-height: 40rpx;
}
.notice-attention{
    margin: 10rpx 48rpx;
    font-size: 24rpx;
    font-family: PingFangSC, PingFang SC,sans-serif;
    font-weight: 400;
    color: #888888;
    line-height: 40rpx;
}

.container-photo{
    display:flex;
    flex-direction:column;
    background: #FFFFFF;
    margin-top: 20rpx;
}

.photo-img{
    display:flex;
    flex-direction:row;
    width: 300rpx;
    margin: 10rpx 28rpx;
    align-items: center;
}
.photo-img >image{
    width: 24rpx;
    height: 26rpx;
    margin-right: 10rpx;
}
.photo-img >text{
    font-size: 28rpx;
    font-family: PingFangSC, PingFang SC,sans-serif;
    font-weight: 500;
    color: #000000;
}
.detail-img{
    padding: 28rpx;
    display:flex;
    flex-direction:column;
    align-items: center;
}
.swiper-photo{
    width: 100%;
    height: 518rpx;
}
.see-more{
    width: 202rpx;
    height: 60rpx;
    background: linear-gradient(180deg, #515151 0%, #333333 100%);
    border-radius: 30rpx;
    border: 2rpx solid;
    text-align: center;
    align-items: center;
    margin-top: 18rpx;

}
.see-text {
    padding-top: 5rpx;
}
.see-text text{
    width: 112rpx;
    height: 40rpx;
    font-size: 28rpx;
    font-family: PingFangSC, PingFang SC,sans-serif;
    font-weight: 500;
    color: #33B670;
    line-height: 40rpx;
    background: linear-gradient(180deg, #3EE88B 0%, #00BC6F 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;

}
.tab-wo{
    width: 100%;
    position: fixed;
    bottom: 0;
}
